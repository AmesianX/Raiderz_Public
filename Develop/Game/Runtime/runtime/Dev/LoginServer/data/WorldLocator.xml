<?xml version="1.0" encoding="utf-8" ?>
<Maiet>
  <!--    
    FILTER: 전체 제한 규칙
      term_sec: 검사 간격, 기본 1초
      request: 요청횟수 제한, 기본 0개(제한 없음)
      byte: 요청용량 제한, 기본 0 bytes(제한 없음)
    IP: IP당 제한 규칙
  -->
  <FILTER term_sec="1" request="0" byte="200000">
    <IP request="100" byte="0"/>
  </FILTER>

  
  <!--    
    IPTABLE: 차단목록
    BLOCK: 차단IP
    IGNORE: 차단예외IP
      ip: 목적 IP
      to_ip: 기술되면, ip ~ to_ip까지 범위로 처리
  -->
  <IPTABLE>    
    <!--<BLOCK ip="172.0.0.1" to_ip="172.0.1.0"/>-->
    <!--<IGNORE ip="172.0.0.2"/>-->
  </IPTABLE>
</Maiet>
