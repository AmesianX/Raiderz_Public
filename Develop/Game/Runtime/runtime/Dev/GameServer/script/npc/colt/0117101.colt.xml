			</CHECK>

		</COMBAT>
		<IDLE>
			<DEFAULT>
				<ACTION type="talent" param1="211710104" rate="10" desc="idle2" cycle="8" />
				<ACTION type="talent" param1="211710105" rate="10" desc="idle3" cycle="8"  />
				<ACTION type="nothing" param1="2" rate="30" />
			</DEFAULT>
		</IDLE>
	</COLT>
	
	<SCRIPT><![CDATA[
	]]></SCRIPT>
</maiet>
