			<DEFAULT>
				<ACTION type="talent" param1="211710101" desc="일반공격1" rate="50"/>
				<ACTION type="talent" param1="211710103" desc="일반공격2" rate="50" />				
			</DEFAULT>
			
			<CHECK type="unbreakpart" param1="2">
				<ACTION type="talent" param1="211710101" desc="일반공격1" rate="30"/>
