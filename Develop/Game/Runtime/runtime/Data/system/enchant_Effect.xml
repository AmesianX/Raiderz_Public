    // 강화 피격 이펙트
    <HitEffect EffectName=""/>
    
     * EffectName               : 이펙트 이름 (effect.xml)

  </DEFAULT>
  
  <EQUIP Type="1hs">
  
     * Type                     : 장착 타입(1hs, 1hb, 1hp, 2hd, sta, arc, 2hb, dwd, dwp, head, body, hands, leg, feet)
  
     ...
  
  </EQUIP>

</ENCHANT_EFFECT>

