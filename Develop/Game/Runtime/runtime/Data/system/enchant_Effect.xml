    <DummyEffect EffectName="" BoneName="" />
    
     * EffectName               : 이펙트 이름 (effect.xml)
     * BoneName                 : 본 이름(없을때에는 전체 본으로 인식)

    // 검광 텍스쳐 이펙트
    <TrailEffect TexName="trail_test1.tga"/>
    
